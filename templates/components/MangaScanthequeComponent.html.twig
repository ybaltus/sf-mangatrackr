<div{{ attributes }}>
    <!-- component html -->
    <section
            id="scantheque-section-{{ statusTrack }}"
            class="bg-app-clear-white dark:bg-app-dark-gray1"
    >
        <div class="py-24 px-8 mx-auto max-w-screen-xl lg:py-24 lg:px-6">
            <div class="text-center lg:text-left mb-section-h2-mob-bottom lg:mb-section-h2-desk-bottom">
                <h2
                        id="title-{{ statusTrack }}"
                        class="text-h2-mob lg:text-h2-desk tracking-normal font-sora-semiBold text-app-dark-gray1 dark:text-app-clear-white"
                >
                    {{ title }} (<span></span>)
                </h2>
            </div>
            <div
                    {{ stimulus_target('scantheque',  statusTrack) }}
                    id="scantheque-list-{{ statusTrack }}"
                    class="grid gap-4 grid-cols-1 lg:grid-cols-2"
            >
            </div>
        </div>
    </section>
</div>