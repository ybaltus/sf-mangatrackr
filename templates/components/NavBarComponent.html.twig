<div{{ attributes }}>
    <!-- component html -->
    <nav
            class="z-50 w-full mx-auto py-2 px-2 lg:px-0 lg:py-4 bg-app-clear-gray dark:bg-app-dark-black  border-b border-hr-clear-mob lg:border-hr-clear-desk dark:border-hr-dark"
    >
        <div class="flex flex-wrap items-center justify-between mx-auto max-w-screen-xl p-4">
            <a
                    href="{{ path('home_index') }}"
                    class="flex items-center space-x-3 rtl:space-x-reverse"
                    title="{{ app_name }}"
            >
                <span
                        class="self-center text-logo-mob lg:text-logo-desk font-sora-semiBold text-transparent bg-clip-text bg-gradient-to-r from-app-clear-purple2 to-app-clear-purple"
                >{{ appName }}</span>
            </a>
            {# Menu Right #}
            <div class="flex items-center lg:order-2 space-x-icon-navbar-mob lg:space-x-icon-navbar-desk">
                {# DarkMode Btn #}
                <button
                        id="theme-toggle" type="button"
                        class="text-app-clear-black dark:text-app-clear-white rounded-lg text-sm"
                >
                    <svg id="theme-toggle-dark-icon" class="hidden w-icon-nav-mode-mob h-icon-nav-mode-mob lg:w-icon-nav-mode-desk lg:h-icon-nav-mode-desk" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M17.293 13.293A8 8 0 016.707 2.707a8.001 8.001 0 1010.586 10.586z"></path></svg>
                    <svg id="theme-toggle-light-icon" class="hidden w-icon-nav-mode-mob h-icon-nav-mode-mob lg:w-icon-nav-mode-desk lg:h-icon-nav-mode-desk" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 2a1 1 0 011 1v1a1 1 0 11-2 0V3a1 1 0 011-1zm4 8a4 4 0 11-8 0 4 4 0 018 0zm-.464 4.95l.707.707a1 1 0 001.414-1.414l-.707-.707a1 1 0 00-1.414 1.414zm2.12-10.607a1 1 0 010 1.414l-.706.707a1 1 0 11-1.414-1.414l.707-.707a1 1 0 011.414 0zM17 11a1 1 0 100-2h-1a1 1 0 100 2h1zm-7 4a1 1 0 011 1v1a1 1 0 11-2 0v-1a1 1 0 011-1zM5.05 6.464A1 1 0 106.465 5.05l-.708-.707a1 1 0 00-1.414 1.414l.707.707zm1.414 8.486l-.707.707a1 1 0 01-1.414-1.414l.707-.707a1 1 0 011.414 1.414zM4 11a1 1 0 100-2H3a1 1 0 000 2h1z" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
                </button>

                {# UserMenu #}
                <button
                        type="button"
                        class="flex text-sm bg-app-clear-white dark:bg-app-dark-black rounded-full md:me-0"
                        id="user-menu-button"
                        aria-expanded="false"
                        data-dropdown-toggle="user-dropdown"
                        data-dropdown-placement="bottom"
                        data-dropdown-offset-distance="15"
                        data-dropdown-offset-skidding="-30"
                >
                    <span class="sr-only">Open user menu</span>
                    {% if app.user %}
                        <div class="relative">
                            <svg
                                    class="w-icon-nav-user-mob h-icon-nav-user-mob lg:w-icon-nav-user-desk lg:h-icon-nav-user-desk text-app-clear-black dark:text-app-clear-white"
                                    aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 14 18">
                                <path d="M7 9a4.5 4.5 0 1 0 0-9 4.5 4.5 0 0 0 0 9Zm2 1H5a5.006 5.006 0 0 0-5 5v2a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2a5.006 5.006 0 0 0-5-5Z"/>
                            </svg>
                            <span class="bottom-0 left-4 lg:left-3 absolute w-4 h-4 lg:w-4 lg:h-4 bg-green-400 border-2 border-white dark:border-gray-800 rounded-full"></span>
                        </div>
                    {% else %}
                        <svg
                                class="w-icon-nav-user-mob h-icon-nav-user-mob lg:w-icon-nav-user-desk lg:h-icon-nav-user-desk text-app-clear-black dark:text-app-clear-white"
                                aria-hidden="true" xmlns="http://www.w3.org/2000/svg" fill="currentColor" viewBox="0 0 14 18">
                            <path d="M7 9a4.5 4.5 0 1 0 0-9 4.5 4.5 0 0 0 0 9Zm2 1H5a5.006 5.006 0 0 0-5 5v2a1 1 0 0 0 1 1h12a1 1 0 0 0 1-1v-2a5.006 5.006 0 0 0-5-5Z"/>
                        </svg>
                    {% endif %}

                </button>
                <div
                        class="z-50 hidden font-sora-regular text-title-menu-mob lg:text-title-manga-desk list-none bg-white divide-y divide-gray-100 rounded-lg shadow dark:bg-gray-700 dark:divide-gray-600"
                        id="user-dropdown"
                >
                    {% if app.user %}
                        <ul class="py-2" aria-labelledby="user-menu-button">
                            {% if is_granted('ROLE_ADMIN') %}
                                <li>
                                    <a href="{{ path('admin_index') }}"
                                       class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:hover:bg-gray-600 dark:text-gray-200 dark:hover:text-white"
                                       title="Espace administrateur"
                                    >
                                        Espace admin
                                    </a>
                                </li>
                            {% endif %}
                            <li>
                                <a href="{{ path('user_index') }}"
                                   class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:hover:bg-gray-600 dark:text-gray-200 dark:hover:text-white"
                                   title="Mon compte"
                                >
                                    Mon compte
                                </a>
                            </li>
                            <li>
                                <a href="{{ path('security_logout') }}"
                                   class="block px-4 py-2 text-gray-700 hover:bg-gray-100 dark:hover:bg-gray-600 dark:text-gray-200 dark:hover:text-white"
                                   title="Se déconnecter"
                                >
                                    Se déconnecter
                                </a>
                            </li>
                        </ul>
                    {% else %}
                        <div class="px-4 py-3">
                            <a
                                    href="{{ path('security_login') }}"
                                    class="block text-gray-900 dark:text-white"
                                    title="Se connecter"
                            >
                                Se connecter
                            </a>
                        </div>
                    {% endif %}
                </div>

                {# Mobile burger menu#}
                <button
                        data-collapse-toggle="mobile-menu-2"
                        type="button"
                        class="inline-flex items-center text-sm text-app-clear-black dark:text-app-clear-white rounded-lg lg:hidden hover:bg-app-clear-gray dark:hover:bg-app-dark-black focus:text-app-focus-purple dark:focus:text-app-focus-purple"
                        aria-controls="mobile-menu-2"
                        aria-expanded="false">
                    <span class="sr-only">Open main menu</span>
                    <svg class="w-icon-nav-menu-mob h-icon-nav-menu-mob" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M3 5a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 10a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1zM3 15a1 1 0 011-1h12a1 1 0 110 2H4a1 1 0 01-1-1z" clip-rule="evenodd"></path></svg>
                    <svg class="hidden w-14 h-14" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path fill-rule="evenodd" d="M4.293 4.293a1 1 0 011.414 0L10 8.586l4.293-4.293a1 1 0 111.414 1.414L11.414 10l4.293 4.293a1 1 0 01-1.414 1.414L10 11.414l-4.293 4.293a1 1 0 01-1.414-1.414L8.586 10 4.293 5.707a1 1 0 010-1.414z" clip-rule="evenodd"></path></svg>
                </button>
            </div>
            {# Main menu #}
            <div
                    class="hidden justify-between items-center w-full lg:flex lg:w-auto lg:order-1 h-screen lg:h-0"
                    id="mobile-menu-2"
            >
                <ul
                        class="flex flex-col lg:flex-row font-sora-semiBold bg-app-dark-gray2-50 mt-menu-mob-top lg:mt-menu-desk-top space-y-menu-navbar-y-mob lg:space-y-menu-navbar-y-desk lg:space-x-menu-navbar-x"
                >
                    {# menus #}
                    {% for key, menu in menus %}
                        {% if app.current_route == key %}
                            <li>
                                <a href="{{ path(key) }}"
                                   class="block font-sora-semiBold text-title-menu-mob lg:text-title-manga-desk text-app-clear-black dark:text-app-clear-white rounded bg-transparent underline underline-offset-menu-mob lg:underline-offset-menu-desk decoration-menu-mob lg:decoration-menu-desk decoration-app-focus-purple"
                                   aria-current="page"
                                   title="{{ menu }}"
                                >
                                    {{ menu }}
                                </a>
                            </li>
                        {% else %}
                            <li>
                                <a href="{{ path(key) }}"
                                   class="block text-title-menu-mob lg:text-title-manga-desk text-app-clear-black border-b hover:bg-app-clear-gray lg:hover:bg-transparent lg:border-0 lg:hover:text-app-clear-black lg:dark:hover:text-app-focus-purple dark:text-app-dark-gray2 border-transparent no-underline"
                                   title="{{ menu }}"
                                >
                                    {{ menu }}
                                </a>
                            </li>
                        {% endif %}
                    {% endfor %}
                </ul>
            </div>
        </div>
    </nav>
</div>
